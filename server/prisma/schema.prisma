datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Puzzle {
  id              Int    @id @default(autoincrement())
  puzzleId        String @unique
  fen             String
  moves           String
  rating          Int
  ratingDeviation Int
  popularity      Int
  nbPlays         Int
  themes          String
  gameUrl         String
  openingTags     String
}